Ransom Note https://leetcode.com/problems/ransom-note/description/
class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        hashmap= {}

        # for i in magazine:
        #     if i in hashmap:
        #         hashmap[i]+=1
        #     else:
        #         hashmap[i]=1
        for i in magazine:
            hashmap[i]=1+hashmap.get(i,0)

        
        # for i in ransomNote:
        #     if i in hashmap:
        #         hashmap[i]-=1
        #         if hashmap[i]<0:
        #             return False
        #     if i not in hashmap:
        #         return False
        # return True
        for i in ransomNote:
            if i not in hashmap or hashmap[i]<=0:
                return False
            hashmap[i]-=1
        return True
            


        
